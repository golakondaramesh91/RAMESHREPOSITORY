{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rameshadfnew"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"LS_ONPREMTOAZURE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ONPREMTOAZURE'"
		},
		"ls_onpremsqltocloud_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_onpremsqltocloud'"
		},
		"ls_onpremsqltocloud_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_onpremsqltocloud'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rameshstoragenew.dfs.core.windows.net"
		},
		"ls_onpremsqltocloud_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "RAMESH REDDY"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rameshadfnew_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"LS_ONPREMTOAZURE_connectionString": {
						"value": "[parameters('LS_ONPREMTOAZURE_connectionString')]"
					},
					"ls_onpremsqltocloud_connectionString": {
						"value": "[parameters('ls_onpremsqltocloud_connectionString')]"
					},
					"ls_onpremsqltocloud_password": {
						"value": "[parameters('ls_onpremsqltocloud_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_onpremsqltocloud_properties_typeProperties_userName": {
						"value": "[parameters('ls_onpremsqltocloud_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}